<div class="play-button"></div>
<div class="song-info">
  <h2 class="song-title"><%= song.escape('title') %></h2>
  <p class="song-uploader"><%= song.escape('uploader') %></p>
  <% if (song.get('tags')) { %>
    <p class="tags">
      <% song.get('tags').forEach(function (tag) { %>
        <span class="tag"><%= tag.label %></span>
      <% }) %>
    </p>
  <% } %>
</div>
